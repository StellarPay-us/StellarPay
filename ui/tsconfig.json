{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#app": ["./.nuxt/types/app"],
      "#imports": ["./.nuxt/types/nitro"]
    },
    "types": ["@nuxt/types"],
    "strict": true
  },
  "include": [
    "types/**/*.d.ts",
    "nuxt.config.ts",
    "app/**/*.ts",
    "pages/**/*.ts",
    "components/**/*.ts",
    "store.ts"
  ]
}
